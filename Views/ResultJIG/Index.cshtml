                        <td>@Html.DisplayFor(modelItem => item.fld_remarks)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_passfail)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_imte)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_dateRecv)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_calibDate)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_calibMonth)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_calibYear)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_nextCalibDate)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_nextCalibMonth)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_nextCalibYear)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_dateReturned)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_internalExternal)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_withNC)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_CalibFR)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_calibDisSusForm)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_withCalibResult)</td>
                        <td hidden>@Html.DisplayFor(modelItem => item.fld_pathDoc)</td>
                        <td hidden>@Html.DisplayFor(modelItem => item.fld_pathIMG)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_incharge)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_changeSticker)</td>
                        <td>@Html.DisplayFor(modelItem => item.fld_actualCalibDueDate)</td>
                        <td hidden>@Html.DisplayFor(modelItem => item.fld_month)</td>
                        <td hidden>@Html.DisplayFor(modelItem => item.fld_year)</td>

                    </tr>
                }
            </tbody>
            <tfoot>
                <tr>
                    <th></th>
                    <th>@Html.DisplayNameFor(model => model.fld_stat)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_ctrlNo)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_jigName)</th>
                    <th>@Html.DisplayNameFor(model => model.calibrationdate)</th>
                    <th hidden>@Html.DisplayNameFor(model => model.fld_codeNo)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_drawingNo)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_term)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_reqFunction)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_remarks)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_passfail)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_imte)</th>
                    <th>@Html.DisplayNameFor(model => model.fld_dateRecv)</th>
